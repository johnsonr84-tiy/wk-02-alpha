Python (function)

function funcDeclaration(para) {
  console.log('funcDeclaration called with', para);
}

var funcExpresssion = function (param) {
  console.log('funcExpression called with', param);
}

const fatArrowFunc1 = param => console.log('fatArrowFunc1 called with, param');

const fatArrowFunc2 = (param1, param2) => {
  console.log('fatArrowFunc2 called with', param1, 'and', param2);
}

funcDeclaration("argument");
funcExpression("arguemtent");
fatArrowFunc1("argument");
fatArrowFunc2("arg1", "arg2");

-Rule: "function declarations" can be positioned anywhere

