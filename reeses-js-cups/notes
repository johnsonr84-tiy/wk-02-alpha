Python (var, let and const)

let letStr = 'let str';
const constStr = 'const str';

console.log(varStr);
console.log(letStr);
console.log(constStr);

varStr = 'new var str';
letstr = 'new let str';

console.log(varStr);
console.log(letStr);
console.log(constStr);

var varStr = 'var str';

